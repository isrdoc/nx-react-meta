{
  "root": "apps/react-app-e2e-pw",
  "sourceRoot": "apps/react-app-e2e-pw/src",
  "projectType": "application",
  "targets": {
    "e2e": {
      "executor": "@ns3/nx-jest-playwright:jest-playwright",
      "options": {
        "devServerTarget": "react-app:serve",
        "jestConfig": "apps/react-app-e2e-pw/jest.config.js",
        "passWithNoTests": true
      },
      "configurations": {
        "production": {
          "devServerTarget": "react-app:serve:production"
        }
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/react-app-e2e-pw/**/*.{ts,tsx,js,jsx}"]
      }
    }
  },
  "tags": [],
  "implicitDependencies": ["react-app"]
}
